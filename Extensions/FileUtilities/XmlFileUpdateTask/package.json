{
  "name": "FileUtilities",
  "scripts": {
    "build": "npm run clean && npm run transpile && npm run package",
    "package": "npm prune -production && npm run copy",
    "copy": "ncp ./node_modules ./dist/node_modules",
    "clean": "rimraf ./dist/*.js && rimraf ./dist/*.js.map && rimraf ./dist/node_modules",
    "cleanmodules": "rimraf ./dist/node_modules",
    "transpile": "tsc -p ./",
    "lint": "tslint -c tslint.json \"src/*.ts",
    "test": "mocha -r ts-node/register ./test/*.ts"
  },
  "dependencies": {
    "@types/node": "^7.0.46",
    "@types/q": "^1.0.6",
    "@types/xmldom": "^0.1.29",
    "Buffer": "^0.0.0",
    "fs": "0.0.1-security",
    "ncp": "^2.0.0",
    "request": "^2.80.0",
    "tsd": "^0.6.5",
    "vsts-task-lib": "^1.1.0",
    "xmldom": "^0.1.27",
    "xpath": "0.0.24"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.44",
    "@types/node": "^7.0.18",
    "chai": "^3.5.0",
    "mocha": "^3.5.3",
    "rimraf": "^2.5.4",
    "sinon": "^2.2.0",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.2.0",
    "typescript": "^2.6.1",
    "typings": "^2.1.1"
  }
}

{
 "manifestVersion": 1,
 "id": "BM-VSTS-XplatGenerateReleaseNotes",
 "version": "2.0.0",
 "name": "Generate Release Notes (Crossplatform)",
 "publisher": "richardfennellBM",
  "description": "Generates a Markdown Release notes file.",
 "categories": [
    "Build and release"
  ],
  "galleryFlags": [
        "Public"
    ],
  "targets": [
         {
             "id": "Microsoft.VisualStudio.Services"
         }
     ],
  "tags": [
    "Release",
    "Package",
    "Publish"
  ],   
  "branding": {
        "color": "#f97923",
        "theme": "dark"
  },
  "icons": {
      "default": "images/logo.png"
  },
   "links": {
    "getstarted": {
      "uri": "https://github.com/rfennell/vNextBuild/wiki/XplatGenerateReleaseNotes"
    },
    "support": {
      "uri": "https://github.com/rfennell/vNextBuild/issues"
    }
  },
  "screenshots": [
    {
      "path": "images/screenshot.png"
    }
  ],
  "content": {
    "details": {
      "path": "readme.md"
    },
    "license": {
      "path": "license.md"
    }
  },
  "files": [
    {
      "path": "_build/dist",
      "packagePath": "/task"
    }
  ],
  "contributions": [
    {
      "id": "XplatGenerate-Release-Notes",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "task"
      }
    }
  ]
}
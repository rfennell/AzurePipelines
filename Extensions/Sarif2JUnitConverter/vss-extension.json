    "support": {
      "uri": "https://github.com/rfennell/AzurePipelines/issues"
    },
    
    "privacypolicy": {
      "uri": "https://github.com/rfennell/AzurePipelines/blob/add-privacy-statement/Extensions/Sarrif2JUnitConverte/privacy.md"
